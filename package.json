{
  "name": "beeline-webcomponents",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build:scss": "node build-scss.js",
    "watch:scss": "nodemon --watch src/webcomponents/**/css/*.scss --exec 'node build-scss.js'",
    "dev": "npm run build:scss && concurrently \"vite\" \"npm run watch:scss\"",
    "build": "vite build",
    "preview": "vite preview",
    "build:tokens": "node tokens/build-tokens.js"
  },
  "dependencies": {
    "@material/web": "^1.4.0",
    "uuid": "^9.0.1",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tokens-studio/sd-transforms": "^0.12.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.3",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "glob": "^10.3.12",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.3",
    "sass": "^1.75.0",
    "style-dictionary": "^3.9.2",
    "vite": "^5.2.0",
    "vitest": "^1.0.4"
  }
}
